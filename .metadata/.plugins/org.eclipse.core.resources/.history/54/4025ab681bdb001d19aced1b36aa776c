package com.hanati.library.domian.repository;

import java.util.List;

import com.hanati.library.domian.model.Book;

public interface BookRepository {

	List<Book> loadBook();

	void save(List<Book> bookList);

	void createMember(Book book);

	void readMember(List<Book> bookList);

	void updateMember(Book book, int id);

	void deleteMember(Book book, int id);


}
