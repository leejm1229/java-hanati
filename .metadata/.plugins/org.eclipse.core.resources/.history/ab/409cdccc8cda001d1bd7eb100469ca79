package com.hanati.library.domian.repository;

import java.util.List;

import com.hanati.library.domain.model.User;
import com.hanati.library.domian.model.Member;

public interface MemberRepository {
	
	List<Member> findAll(Member member);

	void createUser(Member member);

	void updateUser(Member member, int id);

	void deleteUser(Member member, int id);
	
	void rollbackUser(Member member);

	
}
